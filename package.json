{
  "name": "xrm-odata-query-builder",
  "version": "0.0.4",
  "description": "A ts library for generating xrm odata queries for querying Microsoft Dynamics 365 APIs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.2.5"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "test": "jest src",
    "test:watch": "jest --watch src",
    "test:coverage": "jest src --coverage",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukasbaumweb/xrm-odata-query-builder.git"
  },
  "keywords": [
    "xrm",
    "odata",
    "query",
    "builder",
    "microsoft dynamics 365"
  ],
  "author": "lbaum",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lukasbaumweb/xrm-odata-query-builder/issues"
  },
  "homepage": "https://github.com/lukasbaumweb/xrm-odata-query-builder#readme",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
